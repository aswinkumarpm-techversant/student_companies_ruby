<div style="text-align: center">

<%= form_with model: @cash_out_history, local: true do |form| %>

  <% if @cash_out_history.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= pluralize(@cash_out_history.errors.count, "error") %> prohibited
        this History from being saved:
      </h2>
      <ul>
        <% @cash_out_history.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <p>
